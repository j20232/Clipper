{
  "manifest_version": 2,
  "name": "Clipper",
  "description": "",
  "version": "1.0",
  "background": {
      "scripts": [
          "jquery-3.3.1.slim.min.js",
          "background.js"
      ]
  },
  "page_action": {
      "default_icon": {
          "16": "icon.png",
          "24": "icon.png",
          "32": "icon.png"
      },
      "default_title": "clipper",
      "default_popup": "popup.html"
  },
  "icons": {
      "16"  : "icon.png",
      "24"  : "icon.png",
      "32"  : "icon.png",
      "48"  : "icon.png",
      "128" : "icon.png"
  },
  "permissions": [
        "tabs",
        "storage",
		"https://arxiv.org/abs/"
  ],
  "options_ui": {
      "page": "options.html",
      "chrome_style": true
  }
}
